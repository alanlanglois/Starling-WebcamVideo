package starling.extensions.camera.display
{
	/**
	 * ...
	 * @author Alan Langlois
	 */
	public class CameraOrientation 
	{
		
		/**
		 * Specifies that the camera is currently in the default orientation
		 * @langversion	3.0
		 * @playerversion	AIR 2
		 */
		public static const DEFAULT : String = "default";

		/**
		 * Specifies that the camera is currently rotated left relative to the default orientation.
		 * 
		 * @langversion	3.0
		 * @playerversion	AIR 2
		 */
		public static const ROTATED_LEFT : String = "rotatedLeft";

		/**
		 * Specifies that the camera is currently rotated right relative to the default orientation.
		 * 
		 * @langversion	3.0
		 * @playerversion	AIR 2
		 */
		public static const ROTATED_RIGHT : String = "rotatedRight";

		

		/**
		 * Specifies that the camera is currently upside down relative to the default orientation.
		 * @langversion	3.0
		 * @playerversion	AIR 2
		 */
		public static const UPSIDE_DOWN : String = "upsideDown";
	
	}
}
